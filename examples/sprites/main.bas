CHAIN MERGE "sprites.bas"

MODE 1
BORDER 0
FOR Y=8 TO 200 STEP 2
    FOR X=1 TO 300 STEP 2
        RESTORE CLEARSP: call DRAWSPRITE(X,Y,8)
        RESTORE MYSPRITE: call DRAWSPRITE(X+2,Y,8)
        FRAME
    NEXT
    RESTORE CLEARSP: call DRAWSPRITE(X+2,Y,8)
NEXT
END
LABEL MYSPRITE: DATA &C030,&E070,&F4F2,&F0F0,&F0F0,&B4D2,&6861,&C030
LABEL CLEARSP : DATA 0,0,0,0,0,0,0,0